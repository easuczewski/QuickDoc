#include "VersionInfo.xcconfig"

ARCHS = i386 x86_64 armv7 arm64
ARCHS[sdk=iphoneos*] = armv7 arm64
ARCHS[sdk=iphonesimulator*] = i386 x86_64

// Code Signing.
CODE_SIGN_ENTITLEMENTS =

// Packaging.
PRODUCT_NAME = TwilioVideo

// Compiler - Preprocessing.
ENABLE_NS_ASSERTIONS = YES

// Compiler - Language.
GCC_PRECOMPILE_PREFIX_HEADER = NO
GCC_PREFIX_HEADER = TwilioSDK/TwilioSDK-Prefix.pch

OTHER_CFLAGS = -Wall -DWEBRTC_POSIX -DWEBRTC_MAC -DWEBRTC_IOS -DTARGET_OS_IPHONE -DTWILIO_SDK_2_0
OTHER_CFLAGS[sdk=iphoneos*] = $(OTHER_CFLAGS) -DARM

OTHER_LDFLAGS = $(inherited) -lPocoCrypto -lPocoNetSSL -lboringssl -lwebrtc -lwebrtc-objc -lPocoFoundation -lPocoNet -lPocoUtil -lresiprocate -ltwilio-video  -framework CoreTelephony -framework VideoToolbox -framework Foundation -framework CoreText -framework QuartzCore -framework SystemConfiguration -framework CFNetwork -framework UIKit -framework CoreGraphics -framework MobileCoreServices -framework CoreVideo -framework QuickLook -framework AssetsLibrary -framework GLKit -framework OpenGLES -framework CoreMedia -framework CoreImage -framework AudioToolbox -framework AVFoundation -framework Metal -ObjC

HEADER_SEARCH_PATHS = $(SRCROOT)/TwilioSDK/Sources/Objective-C/Implementation $(VIDEO_INCLUDE_PATH) $(VIDEO_INCLUDE_PATH)/video $(VIDEO_INCLUDE_PATH)/third_party $(VIDEO_INCLUDE_PATH)/third_party/webrtc $(VIDEO_INCLUDE_PATH)/third_party/webrtc/third_party/libyuv/include $(VIDEO_INCLUDE_PATH)/third_party/webrtc/webrtc/sdk/objc/Framework/Headers

LIBRARY_SEARCH_PATHS = $(VIDEO_LIB_PATH)

UNEXPORTED_SYMBOLS_FILE=$(PROJECT_DIR)/../target/unexport-$(VIDEO_LIB_TYPE)-$(CURRENT_ARCH).txt
